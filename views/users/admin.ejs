<div class="container">
  <h2 style="text-align: center; margin-bottom: 40px; margin-top: 30px">
    Thông tin tài khoản
  </h2>
  <div
    style="
      display: flex;
      align-items: center;
      justify-content: center;
      margin-bottom: 20px;
    "
  >
    <h5>
      <a href="admin/dmk/<%= objAdmin._id%>" class="dmk">Đổi mật khẩu</a>
    </h5>
    <h5>
      <a href="admin/edit/<%= objAdmin._id%>" class="dmk">Sửa thông tin</a>
    </h5>
  </div>
  <div class="formss" style="width: 500px; margin: auto">
    <form method="post" action="">
      <fieldset>
        <div class="mb-3">
          <label class="form-label">Username</label>
          <input
            type="text"
            id="disabledTextInput"
            class="form-control"
            value="<%= objAdmin.username  %>"
          />
        </div>
        <div class="mb-3">
          <label class="form-label">SĐT</label>
          <input
            type="text"
            id="disabledTextInput"
            class="form-control"
            value="<%= objAdmin.phone  %>"
          />
        </div>
        <div class="mb-3">
          <label class="form-label">Email</label>
          <input
            type="text"
            id="disabledTextInput"
            class="form-control"
            value="<%= objAdmin.email  %>"
          />
        </div>
        <div class="mb-3">
          <label class="form-label">Giới tính</label>
          <input
            type="text"
            id="disabledTextInput"
            class="form-control"
            value="<%= objAdmin.gender  %>"
          />
        </div>

        <div class="mb-3">
          <label class="form-label">Họ tên</label>
          <input
            type="text"
            id="disabledTextInput"
            class="form-control"
            value="<%= objAdmin.fullname  %>"
          />
        </div>
      </fieldset>
    </form>
  </div>
</div>
